    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "# Tạo Series ngẫu nhiên\n",
    "ser = pd.Series(np.random.random(20))\n",
    "\n",
    "# Chia chuỗi thành 10 phân vị bằng nhau và thay thế giá trị bằng tên phân vị\n",
    "ser = pd.qcut(ser, q=10, labels=[f'A{i}' for i in range(1, 11)])\n",
    "\n",
    "print(ser)"
   ]
  }
